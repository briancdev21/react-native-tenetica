    "test": "jest",
    "debug-ios": "open 'rndebugger://set-debugger-loc?host=localhost&port=8081'",
    "debug-windows": "rndebugger-open",
    "android": "yarn android-all",
    "android-all": "concurrently \"npm run android:dubugger\" \"npm run android:packager\" \"cd android && ./gradlew installDebug && cd ..\" \"adb reverse tcp:8081 tcp:8081\"",
